<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.turhanoz.com.services" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="android.turhanoz.com.services.MainActivityfinal"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.turhanoz.com.services.MainActivityfinal" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <service
            android:name=".ServiceA"
            android:label="Service A" >
        </service>
        <service
            android:name=".ServiceB"
            android:label="Service B" >
        </service>
    </application>


</manifest>
